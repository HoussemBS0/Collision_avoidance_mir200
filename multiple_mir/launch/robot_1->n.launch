  <arg name="robot_x"   value="-5.0" />
  <arg name="robot_y"   value="-9.0" />
  <arg name="robot_yaw" value="0" />
  <arg name="displ" value="screen"/>  
</include>

<!-- Setup robot3 mobile robot-->
<include file="$(find multiple_mir)/launch/robots.launch">
  <arg name="robot_name" value="robot3"/>
  <arg name="robot_x"   value="-3.5" />
  <arg name="robot_y"   value="-8.25" />
  <arg name="robot_yaw" value="0" />
